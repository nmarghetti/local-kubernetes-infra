        app.kubernetes.io/instance: mychart
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: play
        app.kubernetes.io/version: 1.0.0
        helm.sh/chart: mychart-0.0.1
        labelKey: labelValue
      name: mychart-play-backend
    spec:
      replicas: 1
      selector:
        matchLabels:
          app.kubernetes.io/component: backend
          app.kubernetes.io/instance: mychart
          app.kubernetes.io/name: play
      template:
        metadata:
          labels:
            app.kubernetes.io/component: backend
            app.kubernetes.io/instance: mychart
            app.kubernetes.io/name: play
        spec:
          affinity:
            nodeAffinity:
              requiredDuringSchedulingIgnoredDuringExecution:
                nodeSelectorTerms:
                  - matchExpressions:
                      - key: kubernetes.io/hostname
                        operator: In
                        values:
                          - worker
          containers:
            - env:
                - name: key
                  value: value
                - name: env-key
                  value: value
                - name: config-key
                  valueFrom:
                    configMapKeyRef:
                      key: some.key
                      name: some-config
              envFrom:
                - configMapRef:
                    name: mychart-play-common
                - configMapRef:
                    name: some-config
                - configMapRef:
                    name: another-config
                - secretRef:
                    name: some-secret
                - secretRef:
                    name: another-secret
                - configMapRef:
                    name: mychart-play-backend
                - configMapRef:
                    name: more-config
                - secretRef:
                    name: more-secret
              image: stefanprodan/podinfo:latest
              imagePullPolicy: IfNotPresent
              livenessProbe:
                httpGet:
                  path: /health
                  port: 8080
              name: app
              ports:
                - containerPort: 9898
                  name: podinfo
                - containerPort: 8080
                  name: http
              resources:
                limits:
                  cpu: 1
                  memory: 200Mi
                requests:
                  cpu: 1
                  memory: 100Mi
              startupProbe:
                failureThreshold: 15
                httpGet:
                  path: /ready
                  port: 8080
                initialDelaySeconds: 1
                periodSeconds: 1
              volumeMounts:
                - mountPath: /usr/share/nginx/html/index.html
                  name: home
                  subPath: index.html
            - image: docker.io/busybox:latest
              imagePullPolicy: Always
              livenessProbe:
                httpGet:
                  path: /ready
                  port: 8080
              name: init
              ports:
                - containerPort: 8080
              resources:
                limits:
                  cpu: 1
                  memory: 200Mi
                requests:
                  cpu: 1
                  memory: 100Mi
          volumes:
            - configMap:
                items:
                  - key: document
                    path: index.html
                name: mychart-play-common
              name: home
