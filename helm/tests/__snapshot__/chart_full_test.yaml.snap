    apiVersion: apps/v1
    kind: Deployment
    metadata:
      annotations:
        annotationKey: annotationValue
        anotherAnnotationKey: anotherAnnotationValue
        configmap.reloader.stakater.com/reload: mychart-play-backend,some-config,another-config,more-config
        kubectl.kubernetes.io/default-container: app
